{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Spring": {
    "Application": {
      "Name": "user.api"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://eurekaserver:8761/eureka/",
      "ShouldFetchRegistry": true,
      "ShouldRegisterWithEureka": true,
      "ValidateCertificates": false
    },
    "Instance": {
      "port": 80,
      "ipAddress": "user.api",
      "preferIpAddress": true
    }
  }
}
