{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "payment.api"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8762/eureka/",
      "shouldFetchRegistry": "true",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {
      "ipAddress": "localhost",
      "preferIpAddress": true
    }
  }
}
